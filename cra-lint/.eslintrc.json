{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "import/no-extraneous-dependencies": [
            "error",
            { "devDependencies": true }
        ]
    }
}
